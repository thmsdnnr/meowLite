{
  "name": "meowLite",
  "version": "0.1",
  "description": "Jump to words in order of frequency as they appear on a webpage. Gaze upon the cute cat in your taskbar.",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "web_accessible_resources": ["script.js", "thescript.js"],
  "icons": {
    "16": "images/cat16.png",
    "32": "images/cat32.png",
    "48": "images/cat48.png",
    "128": "images/cat128.png"
  },
  "manifest_version": 2
}
